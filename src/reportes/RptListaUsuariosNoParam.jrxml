	<queryString>
		<![CDATA[SELECT
     us."nombre",
     us."apellido",
     us."email",
     us."dui",
     us."nip",
     us."telefono",
     CASE us."genero"
     	When 'F' Then 'Femenino'
     	When 'M' Then 'Masculino'
     END as genero,
     tu."tipo_usuario",
     eu."estado_usuario",
     us."fecha_nacimiento"
FROM
     "usuario" us INNER JOIN "tipo_usuario" tu ON us."id_tipo_usuario" = tu."id_tipo_usuario"
     INNER JOIN "estado_usuario" eu ON us."id_estado_usuario" = eu."id_estado_usuario"]]>
	</queryString>
